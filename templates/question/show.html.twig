{% extends 'base.html.twig' %}

{% block title %}Question: {{question}}{% endblock %}

{% block body %}
<h1>{{ question }}</h1>

<div>
	First html file using twig file and variable.
</div>

<h2>Answer ({{ answers|length }})</h2>

<ul>
	{% for answer in answers %}
		<li>{{answer}}</li>
	{% endfor %}

</ul>
{% endblock %}